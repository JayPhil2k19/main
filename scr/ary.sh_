#! /bin/bash

name_str_ary=( "J" "A" "S" "O" "N" )
echo "${name_str_ary[*]}"
echo  "Pick a letter: "
cho=
select "${name_str_ary[*]}"
do
	case $cho in 
	"1") echo "You picked J";;
	"2") echo "You picked A";;
	"3") echo "You picked S";;
	"4") echo "You picked O";;
	"5") echo "You picked N";;
	*) echo "invalid"
	esac
done
